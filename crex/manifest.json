{
  "name": "Bookmark+",
  "version": "1.0",
  "description": "Chrome extension for Bookmark+.",
  "permissions": [
    "tabs"
  ],
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "img/helloworld.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "js/jquery-1.7.1.min.js",
        "js/shortcut.js",
        "js/main.js"
      ]
    }
  ]
}
